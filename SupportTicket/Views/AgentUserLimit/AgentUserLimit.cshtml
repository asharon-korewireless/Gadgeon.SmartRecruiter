
@{
    ViewBag.Title = "AgentUserLimit";
    Layout = "~/Views/Shared/_Layout.cshtml";
        // Layout = null;
}

<script src="~/plugins/jQuery/jQuery-2.1.4.min.js"></script>
<script src="~/js/Masters/AgentUserLimit.js"></script>
<script src="~/js/Custom.js"></script>



<script>

    var api_url = '@ViewBag.api_url'


    $(document).ready(function () {
        ObjAgentUserLimit.init();
    });
</script>
<section class="content-header">
    <h1>AgentUserLimit
            <small>Masters</small>
    </h1>
    <ol class="breadcrumb">
        <li><a href="~/Recruiters/RecruitersDashboard"><i class="fa fa-home"></i>Home</a></li>
        <li class="active"><a href="~/AgentUserLimit/AgentUserLimit">AgentUserLimit</a></li>
    </ol>
</section>

<!-- Main content -->
<section class="content">
    <!-- Small boxes (Stat box) -->
    <div class="row">
        <div class="col-xs-12">
            <div id="ViewBranch">

                <div class="box">
                    <div class="box-header">
                        <h3 class="box-title"></h3>
                        <div class="btn-group pull-left">
                            <button id="btnAddNew" class="btn btn-primary" onclick="ObjAgentUserLimit.Show(2)">
                                Add New <i class="fa fa-plus"></i>
                            </button>
                        </div>
                        <div class="btn-group pull-right">
                            <div class="box-tools">
                                <div class="input-group input-group-sm" style="width: 150px;">
                                    <input name="table_search" class="form-control pull-right" id="txtSearch" placeholder="Search" type="text">

                                    <div class="input-group-btn">
                                        <button type="submit" id="btnSearch" onclick="ObjAgentUserLimit.FillGrid();" class="btn btn-default"><i class="fa fa-search"></i></button>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- /.box-header -->
                    <div id="Grid" class="box-body">
                    </div>
                </div>

            </div>
            <div id="AddBranch" style="display: none;">
                <div class="box">
                    <div class="box-header with-border">
                        <h3 class="box-title">Add AgentUserLimit</h3>
                        <div class="btn-group pull-right">
                            <button id="btnBack" class="btn btn-primary" onclick="ObjAgentUserLimit.init()">
                                <i class="fa fa-arrow-up"></i>
                            </button>
                        </div>
                    </div>
                    <!-- /.box-header -->
                    <!-- form start -->
                    <div class="form-horizontal">
                        <div class="box-body">
                           
                              <div class="form-group">
                                    <label class="col-sm-2 control-label">Company<span style="color: red">*</span></label>
                                    <div class="col-sm-6">
                                         <input type="hidden" id="AgentUserLimitId" value="0" />
                                        <select id="ddlCompanyMaster" class="form-control" onchange="ObjAgentUserLimit.Validate();">
                                        </select>
                                    </div>
                                </div>
                             <div class="form-group">
                                <label class="col-sm-2 control-label">Agent Limit<span style="color: red">*</span></label>
                                <div class="col-sm-6">
                                    <input type="text" class="form-control" id="txtAgentLimit" onkeypress=" CheckInteger(event);" onchange="ObjAgentUserLimit.Validate()"  maxlength="5" />
                                </div>
                            </div>
                                 <div class="form-group">
                                <label class="col-sm-2 control-label">User Limit<span style="color: red">*</span></label>
                                <div class="col-sm-6">
                                    <input type="text" class="form-control" id="txtUserLimit" onkeypress=" CheckInteger(event);" onchange="ObjAgentUserLimit.Validate()"  maxlength="5" />
                                </div>
                            </div>                              

                        </div>
                        <!-- /.box-body -->
                        <div class="box-footer">
                            <div class="btn-group pull-right">
                                <button id="btnSubmit" type="submit" class="btn btn-primary" onclick="ObjAgentUserLimit.Submit()">Submit</button>

                                &nbsp;&nbsp
                           <button type="reset" id="btnClear" class="btn btn-primary" onclick="ObjAgentUserLimit.Clear()">Clear</button>

                            </div>


                        </div>
                        <!-- /.box-footer -->
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- /.row -->


</section>

